{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
	"paths": {
		"@/*":["src/*"],
		"@components/*": ["src/components/*"],
		"@layouts/*": ["src/layouts/*"],
		"@i18n/*": ["src/i18n/*"],
		"@helpers/*": ["src/helpers/*"],
		"@starlight/*": ["node_modules/@astrojs/starlight/*"],
		"@style/*": ["src/style/*"],
		"@utils/*": ["src/utils/*"],
	}
  }
}